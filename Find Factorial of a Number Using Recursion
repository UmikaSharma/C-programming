#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int factorial(int n){
    if(n==1||n==0){
        return 1;
    }
    return n*factorial(n-1);
}
int main() {
    int n;
    scanf("%d",&n);
    int fact=factorial(n);
    printf("%d",fact);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

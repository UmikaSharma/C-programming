#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   int n ,sum=0;
    scanf("%d",&n);
   
    int stu[n];
    for(int i=0;i<n;i++){
        scanf("%d",&stu[i]);
    }
    for(int i=0;i<n;i++){
        sum=sum+stu[i];
       
    }
    printf("%d ",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   int n;
    scanf("%d",&n);
    int ar[n];
    for(int i=0;i<n;i++){
        scanf("%d",&ar[i]);
    }
    int l=ar[0];
    int s=-10001;
    for(int i=0;i<n;i++){
        if(ar[i]>l){
            l=ar[i];
        }
    }
    
    for(int i=0;i<n;i++){
        if(ar[i]!=l && ar[i]>s){
            s=ar[i];             
        }
    }
    if(s==-10001){
      printf("No second largest element");
    }
    else{
  printf("%d", s);
    }
    
    return 0; 
}

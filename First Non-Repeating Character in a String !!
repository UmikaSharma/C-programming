#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[1000];
    scanf("%[^\n]",s);
    char k=0;
    int flag=0;
    for(int i=0;i<strlen(s);i++){
        int count=0;
        for(int j=0;j<strlen(s);j++){
            if(s[i]==s[j]){
                count++;
            }
        }
        if(count==1){
            k=s[i];
            flag=1;
            break;
            
        }
    }
    if(flag==1){
        printf("%c",k);
    }else{
        printf("-1");
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

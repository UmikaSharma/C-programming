#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>
struct student{
    char name[50];
    int age;
    char grade;
};
int main() {
    int n;
    scanf("%d",&n);
    getchar();
    struct student s[n];
    for(int i=0;i<n;i++){
        scanf("%[^\n]",s[i].name);
        getchar();
        scanf("%d",&s[i].age);
        getchar();
        scanf("%c",&s[i].grade);
        getchar();
        
    }
    for(int i=0;i<n;i++){
        printf("Student %d:\n",i+1);
        printf("Name: %s\n",s[i].name);
        if(s[i].age>=3 && s[i].age<=20){
            printf("Age: %d\n",s[i].age);
        }
        if(isupper(s[i].grade)){
            printf("Grade: %c\n",s[i].grade);
            
        }
        
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

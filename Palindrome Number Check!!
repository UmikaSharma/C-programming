#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num,rev=0,l;
    scanf("%d",&num);
        l=num;
    
    if (num>=0 && num<=9){
        printf("Palindrome"); 
    }   
    else
    {
        while(num!=0){
            int rem = num%10;
            rev=rev*10+rem;
            num=num/10;
        }
        if(l==rev)
           printf("Palindrome");
        else
            printf("Not Palindrome");
    }

    
    return 0;
}
    

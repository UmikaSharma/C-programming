#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int count1=0,count2=0;
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n;i++){
        if(arr[i]%2==0){
            count1++;    
        }
        else {
            count2++;
        }
    }
    printf("%d %d",count1,count2);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

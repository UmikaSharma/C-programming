#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int arrA[n];
    int arrB[n];
    int *p1=arrA;
    int *p2=arrB;
    for(int i=0;i<n;i++){
        scanf("%d",p1+i);    
    }
    for(int i=0;i<n;i++){
        scanf("%d",p2+i);
    }
    for(int i=0;i<n;i++){
        int temp=*(p1+i);
        *(p1+i)=*(p2+i);
        *(p2+i)=temp;
    } 
    for(int i=0; i<n; i++) {
        printf("%d ", arrA[i]);
    }
    printf("\n");
    for(int i=0; i<n; i++) {
        printf("%d ", arrB[i]);
    }

    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[1000];
    scanf(" %[^\n]",s);
    for(int i = 0; s[i] != '\0'; i++){
        s[i] = tolower(s[i]);
    }
    char t[1000];
    strcpy(t,s);
    for(int i=0,j=strlen(s)-1;i<j;i++,j--){
        char temp=s[i];
        s[i]=s[j];
        s[j]=temp;
    }
    if(strcmp(t,s)==0){
        printf("Palindrome");
    }else{
        printf("Not Palindrome");
    }
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

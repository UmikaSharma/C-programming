#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int arr[n+1];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    
    int num,index;
    scanf("%d",&num);
    scanf("%d",&index);
    
    for(int i=n;i>=index-1;i--){
        arr[i]=arr[i-1];
    }
    arr[index-1]=num;
    for(int i=0;i<n+1;i++){
        printf("%d ",arr[i]);
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

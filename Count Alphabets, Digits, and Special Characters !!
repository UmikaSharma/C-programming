#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>
int main(){
    int k=0,l=0,t=0;
    char s[1000];
    scanf("%[^\n]",s);
    int n=strlen(s);
    for(int i=0;i<n;i++){
        if(isalpha(s[i])){
            k++;
        }
        else if(isdigit(s[i])){
            l++;
        }else{
            t++;
        }
    }
    printf("%d %d %d",k,l,t);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

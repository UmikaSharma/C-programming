#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int sum(int n){
    if(n==0)return 0;
    return (n%10)+sum(n/10);
}
int main() {
    int n;
    scanf("%d",&n);
    int res=sum(n);
    printf("%d",res);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[1000];
    scanf("%[^\n]",s);
    int n=strlen(s);
    for(int i=0;i<n/2;i++){
        char temp =s[i];
        s[i]=s[n-i-1];
        s[n-i-1]=temp;
    }
    printf("%s",s);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

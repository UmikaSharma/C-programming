#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i,n,sum=0;
    scanf("%d",&i);
    while(i>0){
        n=i%10;
        i=i/10;
        sum=sum+n;
       
    } 
    printf("%d",sum);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

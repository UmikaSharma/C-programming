#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[1000];
    scanf("%[^\n]",s);
    int max=0;
    char t=0;
    for(int i=0;i<strlen(s);i++){
        int count=0;
        for(int j=0;j<strlen(s);j++){
            if(s[i]==s[j]){
                count++;
            }
        }
        if(count>max){
            max=count;
            t=s[i];
        }
    }
    printf("%c occurs %d times",t,max);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
